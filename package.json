{
  "name": "backend-cici",
  "version": "1.0.0",
  "description": "Servicio de api rest para el consumo de datos en cici beauty place",
  "main": "index.js",
  "scripts": {
    "clean:modules": "rm -rf ./node_modules",
    "clean": "rm -rf ./build",
    "prebuild": "yarn clean",
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node build/app.js\"",
    "dev:ts": "ts-node-dev --respawn src/app.ts",
    "format": "prettier --write \"src/**/*.ts\" --write \"package.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" --check \"package.json\"",
    "lint": "eslint . --ext .js,.ts",
    "test": "NODE_ENV=test jest --runInBand --silent --detectOpenHandles",
    "test:watch": "yarn run test --watch",
    "test:coverage": "yarn run test --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GandresCoello18/backend-cici.git"
  },
  "keywords": [
    "cici",
    "beauty",
    "place"
  ],
  "author": "Andres coello goyes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GandresCoello18/backend-cici/issues"
  },
  "homepage": "https://github.com/GandresCoello18/backend-cici#readme",
  "dependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/mysql": "^2.15.17",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.1",
    "@types/pino": "^6.3.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.25.1",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "node-instagram": "^4.3.0",
    "nodemailer": "^6.5.0",
    "pino": "^6.11.0",
    "socket.io": "^4.1.2",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "env-cmd": "10.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "tsc-watch": "4.2.9"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  }
}
